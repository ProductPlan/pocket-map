{
  "compilerOptions": {
    "sourceMap": true,
    "strictNullChecks": false,
    "baseUrl": ".",
    "jsx": "react",
    "outDir": "./dist",
    "allowJs": true,
    "checkJs": false,
    "module": "es6",
    "target": "es6",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "downlevelIteration": true
  },
  "exclude": ["node_modules"],
  "include": ["./src/"]
}
